# Production Environment Configuration
# Copy this file to .env.production and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/mak_realty
POSTGRES_USER=mak_realty_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=mak_realty

# JWT Secret (Generate a strong random string)
JWT_SECRET=your_super_secret_jwt_key_here_minimum_32_characters

# Application URLs
FRONTEND_URL=http://your-ec2-public-ip
REACT_APP_API_URL=http://your-ec2-public-ip:5000

# Server Configuration
NODE_ENV=production
PORT=5000

# AWS Configuration (if using S3 for file uploads - optional)
AWS_REGION=us-east-1
AWS_BUCKET_NAME=mak-realty-uploads
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
